import java.util.Scanner;

public class Practice{
    public static void main(String args[]){
        Scanner input= new Scanner(System.in);
        System.out.print("Enter a number for looping : ");
        int a = input.nextInt();
        int i=0;
        for(i=0;i<=a;i++){
            System.out.println("for loop : "+i);
        }
        
    }
}
